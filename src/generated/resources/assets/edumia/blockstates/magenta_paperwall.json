{
  "multipart": [
    {
      "apply": {
        "model": "edumia:block/magenta_paperwall_post"
      }
    },
    {
      "apply": {
        "model": "edumia:block/magenta_paperwall_side"
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "edumia:block/magenta_paperwall_noside"
      },
      "when": {
        "north": "false"
      }
    },
    {
      "apply": {
        "model": "edumia:block/magenta_paperwall_side_alt"
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "edumia:block/magenta_paperwall_noside_alt",
        "y": 90
      },
      "when": {
        "south": "false"
      }
    },
    {
      "apply": {
        "model": "edumia:block/magenta_paperwall_side_alt",
        "y": 90
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "edumia:block/magenta_paperwall_noside",
        "y": 270
      },
      "when": {
        "west": "false"
      }
    },
    {
      "apply": {
        "model": "edumia:block/magenta_paperwall_side",
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "edumia:block/magenta_paperwall_noside_alt"
      },
      "when": {
        "east": "false"
      }
    }
  ]
}